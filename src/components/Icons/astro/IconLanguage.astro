---
import type { IconProps } from "../types";

const { width, height, color } = Astro.props as IconProps;
const widthValue = width ?? 22;
const heightValue = height ?? 22;
const colorValue = color ?? "#565C78";
---

<svg
  width={widthValue}
  height={heightValue}
  viewBox={`0 0 ${widthValue} ${heightValue}`}
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
>
  <circle cx={10.781} cy={11} r={10} stroke={colorValue} stroke-width={1.5}
  ></circle>
  <ellipse
    cx={10.781}
    cy={11}
    rx={4}
    ry={10}
    stroke={colorValue}
    stroke-width={1.5}></ellipse>
  <path stroke={colorValue} stroke-width={1.5} d="M0.781006 8.25L20.781 8.25"
  ></path>
  <path stroke={colorValue} stroke-width={1.5} d="M0.781006 14.25L20.781 14.25"
  ></path>
</svg>
