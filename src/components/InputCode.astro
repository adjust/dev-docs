---
import { createElement } from "react";
import BuildCustomCode from "./CustomCode";

export interface Props {
  title: string;
  overwriteValue: string;
}

const { title, overwriteValue } = Astro.props as Props;
const placeholder = createElement("span", null, null);
---

<BuildCustomCode
  client:only="react"
  label={title}
  content={placeholder}
  overwriteValue={overwriteValue}
>
  <slot slot="content" />
</BuildCustomCode>
