{
  "routes": [
    {
      "src": "^/en/(.*)",
      "dest": "/en/404",
      "status": 404
    },
    {
      "src": "^/ja/(.*)",
      "dest": "/ja/404",
      "status": 404
    },
    {
      "src": "^/zh/(.*)",
      "dest": "/zh/404",
      "status": 404
    },
    {
      "src": "^/ko/(.*)",
      "dest": "/ko/404",
      "status": 404
    },
    {
      "src": "^/(.*)",
      "dest": "/en/404",
      "status": 404
    }
  ]
}
