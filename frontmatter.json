{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.supportedFileTypes": ["md", "markdown", "mdx", "mdoc"],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "docs",
      "previewPath": "'docs'",
      "pageBundle": false,
      "clearEmpty": true,
      "fileType": "mdoc",
      "fields": [
        {
          "name": "title",
          "type": "string",
          "single": true,
          "default": "Adjust Developer Hub"
        },
        {
          "name": "description",
          "type": "string",
          "single": true,
          "default": "Adjust Developer Hub."
        },
        {
          "name": "sidebar-label",
          "type": "string",
          "single": true
        },
        {
          "name": "sidebar-position",
          "type": "number"
        },
        {
          "name": "category-title",
          "type": "string",
          "single": true
        },
        {
          "name": "navPath",
          "type": "string",
          "single": true
        },
        {
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "name": "ogLocale",
          "type": "string",
          "single": true
        },
        {
          "name": "type",
          "type": "choice",
          "choices": ["category"]
        },
        {
          "name": "multiVersion",
          "type": "boolean"
        },
        {
          "name": "versions",
          "type": "list"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.preview.pathName": "",
  "frontMatter.preview.trailingSlash": false,
  "frontMatter.content.pageFolders": [
    {
      "title": "docs",
      "path": "[[workspace]]/src/content/docs",
      "contentTypes": ["docs"],
      "defaultLocale": "en",
      "locales": [
        {
          "title": "English",
          "locale": "en",
          "path": "en"
        },
        {
          "title": "日本語",
          "locale": "ja",
          "path": "ja"
        },
        {
          "title": "中文",
          "locale": "zh",
          "path": "zh"
        },
        {
          "title": "한국어",
          "locale": "ko",
          "path": "ko"
        }
      ]
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.content.publicFolder": "src/images"
}
